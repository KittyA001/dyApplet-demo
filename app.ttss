}
.c-translucent-balck80 {
  background-color: rgba(0, 0, 0, 0.8);
}
.c-translucent-balck10 {
  background-color: rgba(0, 0, 0, 0.1);
}
.c-translucent-balck30 {
  background-color: rgba(0, 0, 0, 0.3);
}
.c-translucent-blue {
  background-color: rgba(144, 194, 253, 0.1);
}
.c-translucent-red {
  background-color: rgba(253, 144, 144, 0.1);
}

.c-translucent-pink {
  background-color: rgba(250, 198, 178, 0.3);
}
.c-translucent-blue {
  background: rgba(41,121,255,0.10);
}
/*半透明*/

.c-bg-white80 {
  background-color: rgba(255, 255, 255, 0.8);
}

/* 线性渐变 */

.c-linear-blue {
  background: linear-gradient(180deg, #3E86FF, #2979ff);
}
.c-linear-pink {
  background: linear-gradient(180deg, #fac3ad , #ffffff);
}
.c-linear-black {
  background: linear-gradient(360deg, rgba(0, 0, 0, 0.3), rgba(255, 255, 255, 0.1));
}
.c-linear-gray {
  background: linear-gradient(90deg, #919191, #e1e1e1);
}
.c-linear-red {
  background: linear-gradient(180deg,#ff8a7e, #ff6848);
}
.c-linear-xred {
  background: linear-gradient(90deg,#fd4066, #ff493b);
}
.c-linear-sred {
  background: linear-gradient(360deg,#fc4c74, #f6789d);
}
.c-linear-sblue {
  background: linear-gradient(360deg,#6c63fa, #7084f5);
}
.c-course-tag {
  width: 68rpx;
  height: 36rpx;
  background: rgba(0, 0, 0, 0.5);
  border-radius: 6rpx;
  font-size: 24rpx;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
}

.c-course-small-tag {
  width: 56rpx;
  height: 32rpx;
  background: rgba(0, 0, 0, 0.5);
  border-radius: 6rpx;
  font-size: 20rpx;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
}

.c-no-click {
  pointer-events: none;
}

.clearfix:after {
  visibility: hidden;
  display: block;
  font-size: 0;
  content: ' ';
  clear: both;
  height: 0;
}
.tinymce-text{
  position: relative;
  z-index: 1;
}

.tinymce-text img {
  max-width: 100%;
  display: block;
}

.tinymce-text p {
  margin: 0;
}

/* 移动 */
.c-translate-xy-50 {
  transform: translate(-50%, -50%);
}
.c-translate-x-50 {
  transform: translateX(-50%);
}
.c-translate-x100 {
  transform: translateX(100%);
}
.c-translate-y-50 {
  transform: translateY(-50%);
}
@keyframes showOpacity{
  0%{
    opacity: 0;
  }
  100%{
    opacity: 1;
  }
}
@keyframes progress{
  0%{
    width: 0;
  }
  100%{
    width: 100%;
  }
}
@keyframes show{
  0%{
    display: none;
  }
  100%{
    display: block;
  }
}
@keyframes position{
  0%{
    left: -999rpx;
  }
  100%{
    left: 0;
  }
}
