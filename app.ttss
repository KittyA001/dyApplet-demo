.c-animation-opacity{
  opacity: 0;
  animation: showOpacity 0.5s forwards;
}
.c-animation-width{
 /* width: 0; */
 animation: progress 1s linear;
}
.c-animation-position{
  /* width: 0; */
  animation: position 1s linear;
 }
.c-animation-display{
  /* width: 0; */
  animation: show 1s linear;
 }
/*禁止弹窗滚动样式*/

.open-disabled-scroll {
  position: fixed;
  width: 100%;
  overflow: hidden;
  bottom: 0;
}

image {
  max-width: 100%;
  -ms-interpolation-mode: bicubic;
  display: block;
  border: 0;
}

/*半透明*/

.c-translucent-balck60 {
